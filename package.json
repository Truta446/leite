{
  "name": "leite",
  "version": "0.0.3",
  "description": "Gerador de dados brasileiros aleat√≥rios",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/xo && ./node_modules/.bin/ava -v",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report text-lcov -- -R spec | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idwall/leite.git"
  },
  "author": "raphaelgmelo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idwall/leite/issues"
  },
  "homepage": "https://github.com/idwall/leite#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.2",
    "prettier": "^1.3.1",
    "underscore": "^1.8.3",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "moment": "^2.17.0"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "esnext": true,
    "rules": {
      "no-mixed-operators": 0,
      "object-curly-spacing": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run test",
      "prettier --semi false --single-quote --write",
      "git add"
    ]
  }
}
